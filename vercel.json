{
  "version": 2,
  "redirects": [
    {
      "source": "https://ahorra-factura.com/:path*",
      "destination": "https://www.ahorrafactura.com/:path*",
      "permanent": true
    },
    {
      "source": "https://ahorra-factura.vercel.app/:path*",
      "destination": "https://www.ahorrafactura.com/:path*",
      "permanent": true
    }
  ],
  "routes": [
    {
      "src": "/data/(.*)",
      "dest": "/data/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    }
  ],
  "public": true
}