{
  "version": 2,
  "redirects": [
    {
      "source": "https://ahorra-factura.com/:path*",
      "destination": "https://www.ahorrafactura.com/:path*",
      "permanent": true
    },
    {
      "source": "https://ahorra-factura.vercel.app/:path*",
      "destination": "https://www.ahorrafactura.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/data/:path*",
      "destination": "/data/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "/images/:path*"
    },
    {
      "source": "/admin/:path*",
      "destination": "/admin/:path*"
    }
  ],
  "public": true
}