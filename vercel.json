{
  "version": 2,
  "rewrites": [
    {
      "source": "/data/:path*",
      "destination": "/data/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "/images/:path*"
    },
    {
      "source": "/admin/:path*",
      "destination": "/admin/:path*"
    }
  ],
  "public": true
}